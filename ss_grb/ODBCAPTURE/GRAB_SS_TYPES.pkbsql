function dat_phone_list
      (in_pl  in  oe.phone_list_typ)
   return varchar2
is
   delim     varchar2(1);
   ret_txt   varchar2(32767);
begin
   grab_data.grb_cldr_array_lvl := grab_data.grb_cldr_array_lvl + 1;
   delim                        := grab_data.grb_cldr_delim_nt(grab_data.grb_cldr_array_lvl);
