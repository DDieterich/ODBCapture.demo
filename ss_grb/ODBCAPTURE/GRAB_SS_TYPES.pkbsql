function ctl_order_item
      (in_spaces  in  varchar2 default '')
   return varchar2
is
begin
   return 'COLUMN OBJECT TREAT AS OE.ORDER_ITEM_TYP'                                   || CHR(10) || in_spaces ||
          '   (ORDER_ID             FLOAT EXTERNAL TERMINATED BY '';'''                || CHR(10) || in_spaces ||
