is
begin
   return 'COLUMN OBJECT TREAT AS OE.PRODUCT_INFORMATION_TYP'                                                           || CHR(10) || in_spaces ||
          '   (PRODUCT_ID           FLOAT EXTERNAL         TERMINATED BY '';'''                                         || CHR(10) || in_spaces ||
          '   ,PRODUCT_NAME         CHAR(1250)             TERMINATED BY '';'' "UNISTR(to_char(:PRODUCT_NAME))"'        || CHR(10) || in_spaces ||
          '   ,PRODUCT_DESCRIPTION  CHAR(10000)            TERMINATED BY '';'' "UNISTR(to_char(:PRODUCT_DESCRIPTION))"' || CHR(10) || in_spaces ||
          '   ,CATEGORY_ID          FLOAT EXTERNAL         TERMINATED BY '';'''                                         || CHR(10) || in_spaces ||
          '   ,WEIGHT_CLASS         FLOAT EXTERNAL         TERMINATED BY '';'''                                         || CHR(10) || in_spaces ||
          '   ,WARRANTY_PERIOD      INTERVAL YEAR TO MONTH TERMINATED BY '';'''                                         || CHR(10) || in_spaces ||
          '   ,SUPPLIER_ID          FLOAT EXTERNAL         TERMINATED BY '';'''                                         || CHR(10) || in_spaces ||
          '   ,PRODUCT_STATUS       CHAR(20)               TERMINATED BY '';'''                                         || CHR(10) || in_spaces ||
          '   ,LIST_PRICE           FLOAT EXTERNAL         TERMINATED BY '';'''                                         || CHR(10) || in_spaces ||
          '   ,MIN_PRICE            FLOAT EXTERNAL         TERMINATED BY '';'''                                         || CHR(10) || in_spaces ||
          '   ,CATALOG_URL          CHAR(20)               TERMINATED BY '';'''                                         || CHR(10) || in_spaces ||
          '   ,INVENTORY_LIST       ' || grab_ss_types.ctl_inventory_list('INVENTORY_LIST',in_spaces||'   ')            || CHR(10) || in_spaces ||
          '   )';
end ctl_product_information;
-- 
function dat_product_information
      (in_pi  in  oe.product_information_typ)
   return varchar2
is
begin
   return                      to_char(in_pi.product_id)                         ||  ';"' ||
       GRAB_DATA.unistr_encode(replace(in_pi.product_name,';',';;'))             || '";"' ||
       GRAB_DATA.unistr_encode(replace(in_pi.product_description,';',';;'))      || '";'  ||
                               to_char(in_pi.category_id)                        ||  ';'  ||
                               to_char(in_pi.weight_class)                       ||  ';'  ||
                               to_char(in_pi.warranty_period, GRAB_DATA.F_IYTOM) ||  ';'  ||
                               to_char(in_pi.supplier_id)                        ||  ';'  ||
                                       in_pi.product_status                      ||  ';'  ||
                               to_char(in_pi.list_price)                         ||  ';'  ||
                               to_char(in_pi.min_price)                          ||  ';"' ||
                               replace(in_pi.catalog_url,';',';;')               || '";'  ||
      grab_ss_types.dat_inventory_list(in_pi.inventory_list)                     ;
end dat_product_information;

