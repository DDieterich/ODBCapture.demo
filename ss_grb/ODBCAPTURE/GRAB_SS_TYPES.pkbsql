------------------------------------------------------------
function ctl_cust_address
      (in_spaces  in  varchar2 default '')
   return varchar2
is
begin
   return 'COLUMN OBJECT TREAT AS OE.CUST_ADDRESS_TYP'       || CHR(10) || in_spaces ||
          '   (STREET_ADDRESS  CHAR(40) TERMINATED BY '';''' || CHR(10) || in_spaces ||
          '   ,POSTAL_CODE     CHAR(10) TERMINATED BY '';''' || CHR(10) || in_spaces ||
          '   ,CITY            CHAR(30) TERMINATED BY '';''' || CHR(10) || in_spaces ||
          '   ,STATE_PROVINCE  CHAR(10) TERMINATED BY '';''' || CHR(10) || in_spaces ||
          '   ,COUNTRY_ID      CHAR(2)  TERMINATED BY '';''' || CHR(10) || in_spaces ||
          '   )';
end ctl_cust_address;
-- 
function dat_cust_address
      (in_ca  in  oe.cust_address_typ)
   return varchar2
is
begin
   return '"' || replace(in_ca.street_address,';',';;') || '";' ||
                         in_ca.postal_code              || ';' ||
                         in_ca.city                     || ';' ||
                         in_ca.state_province           || ';' ||
                         in_ca.country_id               || ';' ;
end dat_cust_address;

------------------------------------------------------------
function ctl_customer
      (in_spaces  in  varchar2 default '')
   return varchar2
is
begin
   return 'COLUMN OBJECT TREAT AS OE.CUST_ADDRESS_TYP'                                  || CHR(10) || in_spaces ||
          '   (CUSTOMER_ID        FLOAT EXTERNAL TERMINATED BY '';'''                   || CHR(10) || in_spaces ||
          '   ,CUST_FIRST_NAME    CHAR(20)       TERMINATED BY '';'''                   || CHR(10) || in_spaces ||
          '   ,CUST_LAST_NAME     CHAR(20)       TERMINATED BY '';'''                   || CHR(10) || in_spaces ||
          '   ,CUST_ADDRESS       ' || grab_ss_types.ctl_cust_address(in_spaces||'   ') || CHR(10) || in_spaces ||
          '   ,PHONE_NUMBERS      ' || grab_ss_types.ctl_phone_list(in_spaces||'   ')   || CHR(10) || in_spaces ||
          '   ,NLS_LANGUAGE       CHAR(3)        TERMINATED BY '';'''                   || CHR(10) || in_spaces ||
          '   ,NLS_TERRITORY      CHAR(40)       TERMINATED BY '';'''                   || CHR(10) || in_spaces ||
          '   ,CREDIT_LIMIT       FLOAT EXTERNAL TERMINATED BY '';'''                   || CHR(10) || in_spaces ||
          '   ,CUST_EMAIL         CHAR(40)       TERMINATED BY '';'''                   || CHR(10) || in_spaces ||
          '   ,CUST_ORDERS        ' || grab_ss_types.ctl_order_list(in_spaces||'   ')   || CHR(10) || in_spaces ||
          '   )';
end ctl_customer;
-- 
function dat_customer
      (in_cust  in  oe.customer_typ)
   return varchar2
is
begin
   return to_char(in_cust.customer_id)                ||  ';"' ||
          replace(in_cust.cust_first_name,';',';;')   || '";"' ||
          replace(in_cust.cust_last_name,';',';;')    || '";'  ||
          grab_ss_types.dat_cust_address(in_cust.cust_address) ||
          grab_ss_types.dat_phone_list(in_cust.phone_numbers)  ||
                  in_cust.nls_language                ||  ';'  ||
                  in_cust.nls_territory               ||  ';'  ||
          to_char(in_cust.credit_limit)               ||  ';'  ||
                  in_cust.cust_email                  ||  ';'  ||
          grab_ss_types.dat_order_list(in_cust.cust_orders);
end dat_customer;

