function ctl_order
      (in_spaces  in  varchar2 default '')
   return varchar2
is
begin
   return 'COLUMN OBJECT TREAT AS OE.ORDER_TYP'                                         || CHR(10) || in_spaces ||
          '   (ORDER_ID               FLOAT EXTERNAL TERMINATED BY '';'''               || CHR(10) || in_spaces ||
          '   ,ORDER_MODE      CHAR(8)               TERMINATED BY '';'''               || CHR(10) || in_spaces ||
          '   ,CUSTOMER_REF    REF (CONSTANT ''OE.OC_CUSTOMERS'', customer_id)'         || CHR(10) || in_spaces ||
          '   ,CUSTOMER_ID     FILLER FLOAT EXTERNAL TERMINATED BY '';'''               || CHR(10) || in_spaces ||
          '   ,ORDER_STATUS           FLOAT EXTERNAL TERMINATED BY '';'''               || CHR(10) || in_spaces ||
          '   ,ORDER_TOTAL            FLOAT EXTERNAL TERMINATED BY '';'''               || CHR(10) || in_spaces ||
          '   ,SALES_REP_ID           FLOAT EXTERNAL TERMINATED BY '';'''               || CHR(10) || in_spaces ||
          '   ,ORDER_ITEM_LIST ' || grab_ss_types.ctl_order_item_list(in_spaces||'   ') || CHR(10) || in_spaces ||
          '   )';
end ctl_order;
-- 
function dat_order
      (in_ord  in  oe.order_typ)
   return varchar2
is
   cust  oe.customer_typ;
begin
   select DEREF(in_ord.customer_ref) into cust from dual;
   return to_char(in_ord.order_id)      || ';' ||
          to_char(in_ord.order_mode)    || ';' ||
          to_char(cust.customer_id)     || ';' ||
          to_char(in_ord.order_status)  || ';' ||
          to_char(in_ord.order_total)   || ';' ||
          to_char(in_ord.sales_rep_id)  || ';' ||
          grab_ss_types.dat_order_item_list(in_ord.order_item_list) ;
end dat_order;

