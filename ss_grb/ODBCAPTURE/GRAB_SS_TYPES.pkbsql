-- 
function dat_order_item
      (in_oi  in  oe.order_item_typ)
   return varchar2
is
   prod_info  oe.product_information_typ;
begin
   select DEREF(in_oi.product_ref) into prod_info from dual;
   return to_char(in_oi.order_id)       || ';' ||
          to_char(in_oi.line_item_id)   || ';' ||
          to_char(in_oi.unit_price)     || ';' ||
          to_char(in_oi.quantity)       || ';' ||
          to_char(prod_info.product_id) || ';' ;
end dat_order_item;

